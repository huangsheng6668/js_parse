#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_babel-cli@6.26.0@babel-cli/bin/babel-doctor.js" "$@"
  ret=$?
else 
  node  "$basedir/../_babel-cli@6.26.0@babel-cli/bin/babel-doctor.js" "$@"
  ret=$?
fi
exit $ret
